{
	"editor.insertSpaces": false,
	"typescript.tsc.autoDetect": "off",
	"typescript.preferences.quoteStyle": "single",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.semanticTokenColorCustomizations": {
		"enabled" : true,
		"rules": {
			"*.static": {
				"foreground": "#ff0000",
				"fontStyle": "bold"
			},
			"type": {
				"foreground": "#00aa00"
			}, 
			"keyword": "#00fbff"
		}
	}
}